{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "@contents/*": [
        "contents/*"
      ],
      "@groups/*": [
        "groups/*"
      ],
      "@roadmaps/*": [
        "roadmaps/*"
      ],
      "@schedules/*": [
        "schedules/*"
      ],
      "@subject/*": [
        "subject/*"
      ],
      "@users/*": [
        "users/*"
      ],
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
